<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8" />
  <title>Sucursales</title>
  <link rel="stylesheet" href="/styles.css" />
</head>
<body>
  <h1>Sucursales</h1>
  <a href="/">Inicio</a> | <a href="/sucursales/new">Nueva sucursal</a>
  <table border="1" cellpadding="6" cellspacing="0">
    <thead>
      <tr><th>Código</th><th>Dirección</th><th>Teléfono</th></tr>
    </thead>
    <tbody>
      <% for (const it of items) { %>
        <tr>
          <td><%= it.COD_SUCURSAL %></td>
          <td><%= it.DIRECCION %></td>
          <td><%= it.TELEFONO %></td>
        </tr>
      <% } %>
    </tbody>
  </table>
</body>
</html>
